# - Config file for the SpiritSensorGateway package
# It defines the following variables
#  SpiritSensorGateway_INCLUDE_DIRS - include directories for SpiritSensorGateway
#  SpiritSensorGateway_LIBRARIES    - libraries to link against
#  SpiritSensorGateway_EXECUTABLE   - the test executable

# Compute paths
get_filename_component(SpiritSensorGateway_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

include(CMakeFindDependencyMacro)
find_package(uws REQUIRED)

set(SpiritSensorGateway_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# This library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET SpiritSensorGateway AND NOT SpiritSensorGateway_BINARY_DIR)
    include("${SpiritSensorGateway_CMAKE_DIR}/SpiritSensorGatewayTargets.cmake")
endif()

# These are IMPORTED targets created by SpiritSensorGatewayTargets.cmake
set(uws_LIBRARIES uws::uws)
set(SpiritSensorGateway_LIBRARIES SpiritSensorGateway)
set(SpiritSensorGateway_EXECUTABLE runtests)
